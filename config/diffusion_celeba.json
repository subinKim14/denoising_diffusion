{
"dataset": {
    "name": "celeba",
    "path": "../celeba/img_align_celeba",
    "resolution": 128
}
,
"model": {
    "in_channel": 3,
    "channel": 128,
    "channel_multiplier": [1, 1, 2, 2, 4, 4],
    "n_res_blocks": 2,
    "attn_strides": [16],
    "mean_type": "eps",
    "var_type": "fixedlarge",
    "loss_type": "mse",
    "dropout": 0.0,
    "fold": 1,
    "schedule": {
        "type": "linear",
        "beta_start": 1e-4,
        "beta_end": 2e-2,
        "n_timestep": 1000
    },
    "device": "cuda",
    "precision": 32
}
,
"training": {
    "n_iter" : 500000,
    "optimizer": {
        "type": "adam",
        "lr": 2e-5
    },
    "dataloader": {
        "batch_size": 32,
        "num_workers": 2,
        "drop_last": true
    }
}
,
"validation": {
    "dataloader": {
        "batch_size": 32,
        "num_workers": 1,
        "drop_last": false
    }
},
"evaluate": {
    "wandb": false,
    "log_every": 1000,
    "save_every": 5000,
    "valid_every": 5000
}
}
